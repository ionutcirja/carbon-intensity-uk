{
  "verbose": true,
  "clearMocks": true,
  "restoreMocks": true,
  "collectCoverage": true,
  "setupFilesAfterEnv": [
    "./config/enzyme.js"
  ],
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx}",
    "!**/node_modules/**",
    "!src/index.tsx",
    "!**/store/**",
    "!**/reducers/**",
    "!**/routes/**",
    "!**/sagas/**",
    "!**/config/**",
    "!**/style/**",
    "!**/style.*",
    "!coverage/**",
    "!webpack.dev.config.js",
    "!**/server/**"
  ]
}
